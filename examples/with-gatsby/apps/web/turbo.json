{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "pipeline": {
    "build": {
      // include .vercel to ensure output is cached when deploying on Vercel (https://vercel.com/docs/build-output-api/v3)
      "outputs": ["public/**", ".cache/**", ".vercel/**"],
      "env": ["NODE_ENV"]
    }
  }
}
